{
  "name": "kort",
  "scripts": {
    "open": "kort.exe",
    "build:win": "gcc ./src/main.c -o kort.exe -g -O0 -Wall -I./src/include -L./src/lib -lraylib -lgdi32 -lwinmm && yarn open",
